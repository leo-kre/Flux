{
  "expo": {
    "name": "Flux",
    "slug": "Flux",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "platforms": [
      "ios"
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "react-native-health",
        {
          "permissions": {
            "read": [
              "StepCount",
              "DistanceWalkingRunning",
              "DistanceCycling",
              "FlightsClimbed",
              "HeartRate",
              "RestingHeartRate",
              "WalkingHeartRateAverage",
              "HeartRateVariabilitySDNN",
              "VO2Max",
              "ActiveEnergyBurned",
              "BasalEnergyBurned",
              "SleepAnalysis",
              "MindfulSession",
              "BodyMass",
              "Height",
              "BodyMassIndex",
              "BodyFatPercentage",
              "LeanBodyMass",
              "BodyTemperature",
              "RespiratoryRate",
              "OxygenSaturation",
              "BloodPressureSystolic",
              "BloodPressureDiastolic"
            ],
            "write": [
              "Workout"
            ]
          }
        }
      ],
      [
        "react-native-background-fetch",
        {
          "minimumFetchInterval": 15,
          "stopOnTerminate": false,
          "startOnBoot": true,
          "enableHeadless": true,
          "requiredNetworkType": "ANY",
          "scheduleTask": {
            "taskId": "com.deinpaket.health.processing",
            "delay": 60000,
            "periodic": true,
            "stopOnTerminate": false,
            "startOnBoot": true,
            "requiresCharging": false,
            "requiredNetworkType": "ANY"
          }
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true,
      "bundleIdentifier": "com.leokre.flux",
      "infoPlist": {
        "NSHealthShareUsageDescription": "Die App liest Gesundheitsdaten zur Analyse deiner Energie- und Lernfenster.",
        "NSHealthUpdateUsageDescription": "Die App kann Trainingsdaten schreiben (optional).",
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.deinpaket.health.processing"
        ]
      },
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.background-processing": true,
        "com.apple.developer.background-fetch": true
      }
    },
    "android": {
      "package": "com.leokre.flux"
    }
  }
}
